<AnchorPane fx:id="rootView" maxHeight="600.0" maxWidth="1000.0" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Pane fx:id="titlePane" layoutX="-8.0" layoutY="-2.0" prefHeight="50.0" prefWidth="1008.0"
              onMousePressed="#paneMouseClicked" onMouseDragged="#paneMouseDragged">
            <children>
                <Label layoutX="35.0" layoutY="15.0" text="schoen+sandt Alkatrész lista ellenőrző alkalmazás" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="18.0" />
                    </font>
                </Label>
            <Circle fx:id="exitCircle" fill="DODGERBLUE" layoutX="982.0" layoutY="26.0" onMouseExited="#exitCircleOnDragExited"
                    onMouseMoved="#exitCircleOnDrag"  radius="12.0" stroke="BLACK" strokeType="INSIDE" />
                <Label fx:id="exitLabel" layoutX="975.0" layoutY="15.0" onMouseClicked="#startExitDialog" onMouseExited="#exitLabelOnExited" onMouseMoved="#exitLabelOnMoved" text="X" textFill="WHITE">
